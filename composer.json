{
  "name": "redbox/whois",
  "description": "",
  "keywords": [],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Johnny Mast",
      "email": "mastjohnny@gmail.com",
      "homepage": "https://github.com/johnnymast",
      "role": "lead"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-curl": "*"
  },
  "require-dev": {
    "pestphp/pest": "^1.22",
    "squizlabs/php_codesniffer": "^3.7",
    "friendsofphp/php-cs-fixer": "^3.15"
  },
  "autoload": {
    "psr-4": {
      "Redbox\\Whois\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Redbox\\Whois\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest",
    "cs-src": "phpcs src/ --standard=PSR12",
    "cs-tests": "phpcs tests/ --standard=PSR12"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
